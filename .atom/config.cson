    customBackgroundColor: "false"
    fontFamily: "Roboto"
    fontWeight: "Regular"
  linter:
    errorPanelHeight: 46
    showErrorPanel: false
  "linter-eslint": {}
  "markdown-preview-plus": {}
  minimap:
    absoluteMode: true
  "one-light-ui":
    layoutMode: "Compact"
  pigments: {}
  "spell-check": {}
  "unity-ui":
    colorStatusIndicatorsInTreeView: true
    showIcons: true
  welcome:
    showOnStartup: false
".source.ts.tsx":
  editor:
    tabLength: 2
".source.ts":
  editor:
    softWrap: true
    tabLength: 4
".js.source":
  editor:
    tabLength: 4
